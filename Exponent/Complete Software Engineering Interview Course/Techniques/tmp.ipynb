   "source": [
    "from typing import List\n",
    "def insertion_sort(arr: List[int]):\n",
    "    for i in range(1, len(arr)):\n",
    "        value = arr[i]\n",
    "        j = i-1\n",
    "        while j>= 0 and value < arr[j]:\n",
    "            arr[j+1] = arr[j]\n",
    "            j -= 1\n",
    "        arr[j+1] = value\n",
    "    return arr"
   ]
